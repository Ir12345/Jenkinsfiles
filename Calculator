node {git "https://github.com/Ir12345/Calculator.git"def mvnHome = tool 'Maven'sh "'${mvnHome}/bin/mvn' clean site -Dgroups=all"stage('Test') {step([$class: 'Publisher', testResults: '**/testng-results.xml'])}stage(CC') {step([$class: 'JacocoPublisher', execPattern:'**/**.exec', classPattern: '**/classes', sourcePattern: '**/src/main/java'])}}
